void Task_CLOCK()
{
  setup_TFT();
  while(1){
  if (Serial.available() > 0) {
    // Nếu có dữ liệu từ Serial, đọc giờ từ Serial và cập nhật RTC
    int day = Serial.parseInt();
    int month = Serial.parseInt();
    int year = Serial.parseInt();
    int hour = Serial.parseInt();
    int minute = Serial.parseInt();
    int second = Serial.parseInt();

    
    // Chờ đến khi kết thúc dữ liệu được truyền vào
    while (Serial.available() > 0) {
      Serial.read();
      tft.fillRect(40,130, 180, 120, WHITE);
    }

    // Cập nhật thời gian trên module RTC
    rtc.adjust(DateTime(year, month, day, hour, minute, second));
    tft.fillRect(40,130, 180, 120, WHITE);
  }
  
  DateTime now = rtc.now(); //Lấy thời gian hiện tại từ module RTC và lưu vào biến now
//-------Setup vị trí màu sắc và kích thước----------
  tft.setCursor(40, 185);
  tft.setTextColor(RED);
  tft.setTextSize(3);
//--------------Day---------------
  if(now.hour()<1 && now.second()<1 && now.minute()<1 ){
    tft.fillRect(40,185, 30, 20, WHITE);
  }
  tft.print(now.day());
  tft.setCursor(70, 185);
  tft.print(":");
//-------------Month-------------
  tft.setCursor(80, 185);
  if(now.day()==1 && now.hour()<1 && now.minute()<1 && now.second()<1){
    tft.fillRect(80,185, 30, 20, WHITE);
  }
  tft.print(now.month());
  tft.setCursor(115, 185);
  tft.print(":");
//---------------Year---------------
  tft.setCursor(130, 185);
  if(now.day()==1 && now.month()==1 && now.hour()<1 && now.minute()<1){
    tft.fillRect(130,185, 60, 20, WHITE);
  }
  tft.print(now.year());
//---------------Hour----------------
  tft.setTextSize(4);
  tft.setCursor(40, 130);
  if(now.minute()<1){
    tft.fillRect(40,130, 50, 40, WHITE);
  }
  if(now.hour()<=9){
    tft.print("0");
    tft.print(now.hour());
    tft.print(" ");
  } else{
    tft.print(now.hour());
    tft.print(" ");
  }
  tft.setCursor(80, 130);
  tft.print(":");
//--------------Minute--------------
  tft.setCursor(95, 130);
  if(now.second()<=1){
    tft.fillRect(95,130, 50, 40, WHITE);
  }
  if(now.minute()<=9){
    tft.print("0");
    tft.print(now.minute());
    tft.print(" ");
  } else{
    tft.print(now.minute());
    tft.print(" ");
  }
  tft.setCursor(135, 130);
  tft.print(":");
//--------------Second----------------
  tft.setCursor(150, 130);
  tft.fillRect(150,130, 50, 40, WHITE);
  if(now.second()<=9){
    tft.print("0");
    tft.print(now.second());
    tft.print(" ");
  } else{
    tft.print(now.second());
    tft.print(" ");
  }
//--------------DayofWeek---------------
  tft.setTextSize(2);
  tft.setCursor(80,165);
  if(now.hour()<1 && now.second()<=1 && now.minute()<=1 ){
  tft.fillRect(80,165, 150, 20, WHITE);
  }
  tft.print(daysOfTheWeek[now.dayOfTheWeek()]);
  delay(1000);
  }
}



